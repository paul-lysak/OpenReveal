        a ore:PoliticanClaim;

        ore:claimBy <#scribe1>
        ore:claimDateTime "2015-05-29T10:06:00"

        ore:memberOr <#Imperial-Party-Of-Galaxy>
        ore:citizenOf <#Tatouine>
    ]



